package homework5;
import java.util.Scanner;
public class Q1 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("자연수를 입력하시오");
		int num = input.nextInt();
		int count = 0;
		int sum =0;
		
		for(int i =1; sum<num ; i++) {
			if(i%2 !=0) {
				sum = sum +i;
				count++;
			}
		}
		
		
		System.out.println(count + " " +sum);
			
		
		input.close();


	}

}
