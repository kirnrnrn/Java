package extraHomework;

public class miniProjectDice {

	public static void main(String[] args) {
		int a=0;
		int b=0;
		int c=0;
		do {
			a =(int)(Math.random()*6)+1;
			b =(int)(Math.random()*6)+1;
			System.out.println("주사위1=" +a +" " + "주사위2=" +b);
			c++;
		}while(a +b !=2);
	System.out.println("(1,1)이 나오는데 걸린 횟수=" + c);
	}

}
