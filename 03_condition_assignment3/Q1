package homework3;
import java.util.Scanner;
public class Q1 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int a;
		System.out.println("정수를 입력하시오");
		a = input.nextInt();
		
		if (a == 0) {
			System.out.println("zero");
		}
		else if (a>0) {
			System.out.println("plus");
		}
		else if(a<0) {
			System.out.println("minus");
		}
		
		
		
		
		
		
		input.close();
	}

}
