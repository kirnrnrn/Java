package homework2;
import java.util.Scanner;
public class Q4 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int mh;
		int mw;
		int gh;
		int gw;
		
		System.out.println("민수의 키는?");
		mh = input.nextInt();
		System.out.println("민수의 몸무게는?");
		mw = input.nextInt();
		
		System.out.println("기영이의 키는?");
		gh = input.nextInt();
		System.out.println("기영이의 몸무게는?");
		gw = input.nextInt();
		
		if (mh>gh) {
			if (mw> gw) {
				System.out.println("1");
			}
			else {
				System.out.println("0");
			}
		}
		else {
			System.out.println("0");
		}
		
		
		
		input.close();
	}

}
